{
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/js-api",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "src"
    ],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  }
}
